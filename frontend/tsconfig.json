{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "watch": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "src",
    "paths": {
      "@store/*": [
        "app/"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@objects/*": [
        "objects/*"
      ],
      "@app/*": [
        "app/*"
      ],
      "@api/*": [
        "api/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
